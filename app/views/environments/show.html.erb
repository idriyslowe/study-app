<h1>This environment is</h1>
<h1><%= @environments.name %></h1>
<h3><%= current_user.email %></h3>
<ul>
  <% @environments.note_pages.each do |page| %>
    <li><a href="/note_pages/#{page.id}"><%= page.title %></a></li>
    <% page.bookmarks.each do |bookmark| %>
      <%= bookmark.name %>
    <% end %>
  <% end %>
</ul>
<div>
  <%= link_to 'Edit', "/environments/#{@environments.id}/edit" %>
</div>
<div>
  <%= link_to 'Archive', "/environments/#{@environments.id}", method: :delete %>
</div>