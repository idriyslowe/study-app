<h1>Create a new Note Page</h1>
<%= form_tag "/note_pages", method: :post do %>
  <div>
    <%= label_tag :title, "Name" %>
    <%= text_field_tag :title %>
  </div>
  <div>
    <%= label_tag :text, "Text" %>
    <%= text_field_tag :text %>
  </div>
  <div>
  <%= submit_tag 'Send' %>
  </div>
<% end %>